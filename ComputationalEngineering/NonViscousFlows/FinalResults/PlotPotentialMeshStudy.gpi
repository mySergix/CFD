set grid
set xtics font "Arial-Bold, 15"
set ytics font "Arial-Bold, 15"
set key left top font 'Arial-Bold, 20'
set title "Rotating Cylinder Lift" font 'Arial-Bold, 20'
set xlabel "Solid Stream Function Value" font 'Arial-Bold, 20'
set ylabel "Lift [N]" font 'Arial-Bold, 20'
set key right top

plot 'CirculationLift.txt' using 1:2 w lines lw 3 lt rgb "blue" title "Cylinder Circulation Lift",\
    'PressureLift.txt' using 1:2 w lines lw 3 lt rgb "black" title "Cylinder Pressure Lift"
   
   
	

      
